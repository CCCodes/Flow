+
+<dom-module id="unit-sel">
+    <template>
+        <style>
+            :host {
+                display: block;
+                background-color: inherit;
+                margin: 0;
+                padding: 0;
+                border: 0;
+                outline: 0;
+                --iron-icon-height: 20px;
+                --iron-icon-width: 20px;
+                height: 17.5px;
+            }
+
+            #unitselect {
+                display: block;
+                background-color: inherit;
+                border: 0;
+                width: 40px;
+                height: 20px;
+                margin: 0;
+            }
+
+            #unitselect:hover {
+                outline: none;
+            }
+
+            .addon {
+                display: inline-flex;
+                display: -webkit-inline-flex;
+                margin: 0;
+            }
+
+            :host([pressed]) datalist#metric {
+                display: block;
+                position: absolute;
+                top: 0;
+            }
+
+            .showDatalist {
+                position: fixed;
+                margin-top: 35px;
+                border: 0.2px solid #bababa;
+                background: white;
+                width: 60px;
+                height: max-content;
+                height: -webkit-max-content;
+                height: 80px;
+                color: #000;
+                text-align: left;
+                z-index: 10;
+                font-family: sans-serif;
+                padding: 3px;
+                padding-left: 5px;
+            }
+
+            .showDatalist option {
+                font-weight: bold;
+                color: #000 !important;
+                display: block;
+                font-size: 1em;
+                z-index: 11;
+            }
+
+            .showDatalist option:hover {
+                background: #bababa;
+            }
+        </style>
+        <div class="addon">
+            <input type="text" id="unitselect" class="notform" value="{{value}}"/>
+            <iron-icon id="show" icon='icons:chevron-right'></iron-icon>
