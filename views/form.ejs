<body>

    <button id="launchModal" type="button" class="invisible" data-toggle="modal" data-target="#myModal"></button>

    <div class="modal fade" tabindex="-1" role="dialog" id="myModal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">What group are you in?</h4>
                </div>
                <div class="modal-body">
                    <form id="setGroup" name="setGroup" class="form" action="/" method="post">
                        <div class="form-group">
                            <select id="groups" class="form-control">
                                <option value=""></option>
                                <option value="1">I'm in Group 1</option>
                                <option value="2">I'm in Group 2</option>
                                <option value="3">I'm in Group 3</option>
                                <option value="4">I'm in Group 4</option>
                                <option value="5">I'm in Group 5</option>
                                <option value="6">I'm doing every Group</option>
                            </select>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="displaySpecificGroup"  name="displaySpecificGroup" form="setGroup">Continue to Lab</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -s-c -->
    <script>
        $( document ).ready(function() {
            $("#displaySpecificGroup").click(function(){
                var g = $("#groups").val();
                if( g == 1 ){
                    $("#g2, #g3, #g4, #g5").hide()
                }else if( g == 2 ){

                }
            });
        });
    </script>

